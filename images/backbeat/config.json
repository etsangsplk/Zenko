{
    "zookeeper": {
        "connectionString": "zookeeper:2181/backbeat",
        "autoCreateNamespace": true
    },
    "kafka": {
        "hosts": "kafka:9092"
    },
    "extensions": {
        "replication": {
            "source": {
                "transport": "http",
                "s3": {
                    "host": "cloudserver-front",
                    "port": 8000
                },
                "auth": {
                    "type": "account",
                    "account": "bart"
                },
                "logSource": "dmd",
                "dmd": {
                    "host": "cloudserver-metadata",
                    "port": 9990
                }
            },
            "destination": {
                "transport": "http",
                "bootstrapList": [],
                "certFilePaths": {
                    "key": "ssl/key.pem",
                    "cert": "ssl/cert.crt",
                    "ca": "ssl/ca.crt"
                },
                "auth": {
                    "type": "account",
                    "account": "lisa"
                }
            },
            "topic": "backbeat-replication",
            "queuePopulator": {
                "cronRule": "*/5 * * * * *",
                "batchMaxRead": 10000,
                "zookeeperPath": "/replication-populator"
            },
            "queueProcessor": {
                "groupId": "backbeat-replication-group",
                "retryTimeoutS": 300
            }
        }
    },
    "log": {
        "logLevel": "info",
        "dumpLevel": "error"
    }
}
